    <TextView
        android:id="@+id/textView"
        android:layout_width="225dp"
        android:layout_height="60dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="77dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="PROFILE"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="25dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/namaUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/namaUser"
        android:layout_width="300dp"
        android:layout_height="44dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="53dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/noTelpUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        android:layout_marginTop="121dp"
        android:layout_marginEnd="169dp"
        android:layout_marginBottom="12dp"
        android:text="Username dan No.Telp"
        app:layout_constraintBottom_toTopOf="@+id/namaUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/telpLama"
        android:layout_width="300dp"
        android:layout_height="44dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="7dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/noTelpUser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namaUser"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnProf"
        android:layout_width="128dp"
        android:layout_height="69dp"
        android:layout_marginStart="147dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="259dp"
        android:text="Save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/noTelpUser" />

    <EditText
        android:id="@+id/noTelpUser"
        android:layout_width="241dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="42dp"
        android:ems="10"
        android:hint="Edit No telp"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/btnProf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namaUser" />


